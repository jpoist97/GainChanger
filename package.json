{
  "main": "index.js",
  "scripts": {
    "start": "npx tsc --skipLibCheck && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios --simulator=\"iPhone 11 Pro Max\"",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./components --quiet",
    "lint-autofix": "eslint --ext .js,.jsx,.ts,.tsx ./components --quiet --fix",
    "test": "jest --collect-coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|ts|tsx)\"",
    "format-check": "prettier --check \"**/*.+(js|jsx|json|ts|tsx)\"",
    "prepare": "husky install",
    "tsc": "npx tsc --skipLibCheck"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.1.0",
    "@expo-google-fonts/roboto": "^0.1.0",
