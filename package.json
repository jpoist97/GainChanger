    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.12.4",
    "@types/react-native-draggable-flatlist": "^2.0.0",
    "date-fns": "^2.16.1",
    "expo": "^43.0.0",
    "expo-font": "~10.0.3",
    "expo-haptics": "~11.0.3",
    "expo-notifications": "~0.13.3",
